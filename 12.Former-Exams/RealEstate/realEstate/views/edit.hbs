                            <input type="number" class="input-field" id="year" name="year" value="{{data.year}}" />
                            <label class="year">Year Built:</label>
                        </div>
                        <div class="input">
                            <input type="text" class="input-field" id="city" name="city" value="{{data.city}}" />
                            <label class="city">City</label>
                        </div>

                        <div class="input">
                            <input type="text" class="input-field" id="homeImage" name="imageUrl" value="{{data.imageUrl}}" />
                            <label class="homeImage">Home Image:</label>
                        </div>

                        <div class="input">
                            <input type="text" class="input-field" id="description" name="description" value="{{data.description}}" />
                            <label class="description">Property Description:</label>
                        </div>

